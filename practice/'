#include<cstdio>
#include<cstdlib>
#include<cstring>
#include<iostream>

using namespace std;

int main(int argc, char* argv[]) {
    float a = 1.0f;
    cout << boolalpha << ((int)a == (int&)a) << endl;
    
    float aa = 0.0f;
    cout << boolalpha << ((int)aa == (int&)aa) << endl;
    
    return 0;
}



